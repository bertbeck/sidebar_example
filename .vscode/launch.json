    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "pwa-chrome",
            "webRoot": "${workspaceFolder}"
        },

        {
            "name": "flutter_collapsible_sidebar",
            "request": "launch",
            "type": "dart",
            "args": [
                "--no-sound-null-safety",
            ],                
        },
        {
            "name": "example",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "args": [
                "--no-sound-null-safety",
            ],              
        }
    ]
}